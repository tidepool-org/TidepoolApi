        "Test": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "Source": {
          "type": "object",
          "properties": {
            "ID": {
              "type": [
                "string",
                "null"
              ]
            },
            "Name": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        },
        "Destinations": {
          "type": "array",
