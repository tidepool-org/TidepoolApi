        "Message": {
          "type": "object",
          "properties": {
            "ID": {
              "type": [
                "number",
                "null"
              ]
            }
          }
        },
        "Transmission": {
          "type": "object",
          "properties": {
            "ID": {
              "type": [
                "number",
                "null"
              ]
            }
          }
        },
        "FacilityCode": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "DataModel",
        "EventType"
      ]
